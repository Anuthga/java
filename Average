//find the average of 5 numbers by using array
public class Average{
public static void main (String args[]){
System.out.println("find the average of 5 numbers by using array");
double average=0;
int sum=0;

int [] marks;
marks=new int[5];
marks[0]=67;
marks[1]=75;
marks[2]=48;
marks[3]=88;
marks[4]=89;


for(int i=0;i<5;i++)
{
sum=sum+marks[i];
average=(sum/7);
}
System.out.println("Average is " +average);
}
}
